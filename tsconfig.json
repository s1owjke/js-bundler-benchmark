{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["es2023"],
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022"
  },
  "include": ["./scripts"],
  "exclude": ["node_modules"]
}
